package crf.util;

/**
 * 文件操作类
 * @author Administrator
 *
 */
public class FileUtil {
	/**
	 * 	功能:获取文件扩展名
	 *	王浩  @2015-7-26
	 */
	public static String getExtensionName(String fileName) {  
		int dot = fileName.lastIndexOf('.');
		 if ((dot >-1) && (dot < (fileName.length() - 1))){
			 return fileName.substring(dot + 1);
		 }
		 return "";
	}
}
